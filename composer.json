{
  "name": "app/user-activity-tracker",
  "description": "Simple web project with user activity tracking",
  "type": "project",
  "require": {
    "php": "^8.2",
    "symfony/http-foundation": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/routing": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/config": "^6.4",
    "symfony/yaml": "^6.4",
    "symfony/console": "^6.4",
    "symfony/validator": "^6.4",
    "symfony/serializer": "^6.4",
    "symfony/event-dispatcher": "^6.4",
    "symfony/twig-bundle": "^6.4",
    "twig/twig": "^3.7",
    "monolog/monolog": "^3.0",
    "vlucas/phpdotenv": "^5.0",
    "ramsey/uuid": "^4.0",
    "psr/http-message": "^2.0",
    "psr/container": "^2.0",
    "doctrine/dbal": "^3.0",
    "nesbot/carbon": "^2.0",
    "symfony/dotenv": "^6.4",
    "symfony/error-handler": "^6.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "mockery/mockery": "^1.6",
    "phpstan/phpstan": "^1.10",
    "squizlabs/php_codesniffer": "^3.7"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "phpstan": "phpstan analyse src --level=8",
    "cs-check": "phpcs src",
    "cs-fix": "phpcbf src"
  }
}
